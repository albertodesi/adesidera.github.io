<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-profile service-profile-id="AkeneoGeneral">
        <timeout-millis>10000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="AkeneoGetGeneral">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>AkeneoGetGeneral</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>AkeneoGeneral</profile-id>
        <credential-id/>
    </service>

    <service service-id="AkeneoGetToken">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>AkeneoGetToken</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>AkeneoGeneral</profile-id>
        <credential-id/>
    </service>

</services>
